[32m+[m[32m          </conditions>[m
[32m+[m[32m          <action type="Rewrite" url="index.php" />[m
[32m+[m[32m        </rule>[m
[32m+[m[32m      </rules>[m
[32m+[m[32m    </rewrite>[m
[32m+[m[32m  </system.webServer>[m
[32m+[m[32m</configuration>[m
