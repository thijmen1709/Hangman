[32m+[m[32m     */[m
[32m+[m[32m    public function up()[m
[32m+[m[32m    {[m
[32m+[m[32m        Schema::create('failed_jobs', function (Blueprint $table) {[m
[32m+[m[32m            $table->bigIncrements('id');[m
[32m+[m[32m            $table->text('connection');[m
[32m+[m[32m            $table->text('queue');[m
[32m+[m[32m            $table->longText('payload');[m
[32m+[m[32m            $table->longText('exception');[m
[32m+[m[32m            $table->timestamp('failed_at')->useCurrent();[m
[32m+[m[32m        });[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    /**[m
[32m+[m[32m     * Reverse the migrations.[m
[32m+[m[32m     *[m
[32m+[m[32m     * @return void[m
[32m+[m[32m     */[m
[32m+[m[32m    public function down()[m
[32m+[m[32m    {[m
[32m+[m[32m        Schema::dropIfExists('failed_jobs');[m
[32m+[m[32m    }[m
[32m+[m[32m}[m
